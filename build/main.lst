 205:Src/main.c    **** void bubble_sort (int *a, int n) 
 206:Src/main.c    **** {
 207:Src/main.c    ****     int i, t, s = 1;
 208:Src/main.c    ****     globalCounter = 0x0D;
 209:Src/main.c    ****     while (s) 
 210:Src/main.c    ****     {
 211:Src/main.c    ****         s = 0;
 212:Src/main.c    ****         for (i = 1; i < n; i++) 
 213:Src/main.c    ****         {
 214:Src/main.c    ****             if (a[i] < a[i - 1]) 
 215:Src/main.c    ****             {
 216:Src/main.c    ****                 t = a[i];
 217:Src/main.c    ****                 a[i] = a[i - 1];
 218:Src/main.c    ****                 a[i - 1] = t;
 219:Src/main.c    ****                 s = 1;            
 220:Src/main.c    ****             }
 221:Src/main.c    ****         }
 222:Src/main.c    ****     }
 223:Src/main.c    **** }
